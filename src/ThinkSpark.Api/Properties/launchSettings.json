{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "ThinkSpark (Development)": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "APPLICATION_NAME": "ThinkSpark.Api",
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:5099"
    },
    "ThinkSpark (Homolog)": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "APPLICATION_NAME": "ThinkSpark.Api",
        "ASPNETCORE_ENVIRONMENT": "Homolog"
      },
      "applicationUrl": "http://localhost:5099"
    },
    "ThinkSpark (Local)": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "APPLICATION_NAME": "ThinkSpark.Api",
        "ASPNETCORE_ENVIRONMENT": "Local"
      },
      "applicationUrl": "http://localhost:5099"
    }
  }
}












